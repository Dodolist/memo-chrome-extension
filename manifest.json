{
    "name": "MemoLink",
    "description": "Notepad, a Chrome extension that enables synchronization between Google accounts.",
    "version": "1.3.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}
