{
    "name": "MemoLink",
    "description": "Notepad, a Chrome extension that enables synchronization between Google accounts.",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "128.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "128": "128.png"
    }
}
